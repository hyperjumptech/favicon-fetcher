{
  "name": "@hyperjumptech/favicon-fetcher",
  "version": "1.0.1",
  "description": "A utility to fetch a website's favicon",
  "main": "dist/index.js",
  "scripts": {
    "start": "tsc && node dist/index.js",
    "build": "tsc",
    "lint": "eslint . --ext .ts",
    "format": "prettier --write src/*",
    "format:check": "prettier --check src/*",
    "test": "jest --runInBand",
    "test:coverage": "jest --runInBand --coverage",
    "prepare": "husky install"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/hyperjumptech/favicon-fetcher.git"
  },
  "keywords": [
    "favicon",
    "hyperjumptech"
  ],
  "author": "Denny Pradipta",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/hyperjumptech/favicon-fetcher/issues"
  },
  "homepage": "https://github.com/hyperjumptech/favicon-fetcher#readme",
  "devDependencies": {
    "@types/jest": "^29.5.1",
    "@types/node-fetch": "^2.6.4",
    "@typescript-eslint/eslint-plugin": "^5.59.7",
    "eslint": "^8.41.0",
    "eslint-config-standard-with-typescript": "^34.0.1",
    "eslint-plugin-github": "^4.7.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-jest": "^27.2.1",
    "eslint-plugin-n": "^15.7.0",
    "eslint-plugin-promise": "^6.1.1",
    "husky": "^8.0.3",
    "jest": "^29.5.0",
    "prettier": "^2.8.8",
    "ts-jest": "^29.1.0",
    "typescript": "^5.0.4"
  },
  "dependencies": {
    "@types/node": "^20.2.3",
    "debug": "^4.3.4",
    "node-fetch": "^2.6.11"
  }
}
